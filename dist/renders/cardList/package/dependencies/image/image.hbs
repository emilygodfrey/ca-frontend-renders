<div class="amp-ca-image-holder">
<picture class="amp-ca-image">
        {{#if display}}
            {{#iff display '==' 'Static'}}
                <img src="//neildemo.a.bigcontent.io/v1/static/{{image.name}}{{#if seoText}}/{{escapeUrl seoText}}{{/if}}"
                 class="amp-ca-image-pic" {{#if imageAltText}} alt="{{imageAltText}}" {{/if}} {{#if seoText}} title="{{seoText}}"{{/if}} />
            {{/iff}}
        {{else}}
                <source srcset="//{{image.defaultHost}}/i/{{image.endpoint}}/{{image.name}}{{#if seoText}}/{{escapeUrl seoText}}{{/if}}.webp?w=1600{{#roundelProperties}}&$roundel$&{{roundelConfig roundel}}{{/roundelProperties}} 1x, //{{image.defaultHost}}/i/{{image.endpoint}}/{{image.name}}{{#if seoText}}/{{escapeUrl seoText}}{{/if}}.webp?w=3200{{#roundelProperties}}&$roundel$&{{roundelConfig roundel}}{{/roundelProperties}} 2x"
                media="(min-width: 1280px)"
                type="image/webp">
            <source srcset="//{{image.defaultHost}}/i/{{image.endpoint}}/{{image.name}}{{#if seoText}}/{{escapeUrl seoText}}{{/if}}?w=1600{{#roundelProperties}}&$roundel$&{{roundelConfig roundel}}{{/roundelProperties}} 1x, //{{image.defaultHost}}/i/{{image.endpoint}}/{{image.name}}{{#if seoText}}/{{escapeUrl seoText}}{{/if}}?w=3200{{#roundelProperties}}&$roundel$&{{roundelConfig roundel}}{{/roundelProperties}} 2x"
                    media="(min-width: 1280px)">

            <source srcset="//{{image.defaultHost}}/i/{{image.endpoint}}/{{image.name}}{{#if seoText}}/{{escapeUrl seoText}}{{/if}}.webp?w=1280{{#roundelProperties}}&$roundel$&{{roundelConfig roundel}}{{/roundelProperties}} 1x, //{{image.defaultHost}}/i/{{image.endpoint}}/{{image.name}}{{#if seoText}}/{{escapeUrl seoText}}{{/if}}.webp?w=2560{{#roundelProperties}}&$roundel$&{{roundelConfig roundel}}{{/roundelProperties}} 2x"
                    media="(min-width: 1024px)"
                    type="image/webp">
            <source srcset="//{{image.defaultHost}}/i/{{image.endpoint}}/{{image.name}}{{#if seoText}}/{{escapeUrl seoText}}{{/if}}?w=1280{{#roundelProperties}}&$roundel$&{{roundelConfig roundel}}{{/roundelProperties}} 1x, //{{image.defaultHost}}/i/{{image.endpoint}}/{{image.name}}{{#if seoText}}/{{escapeUrl seoText}}{{/if}}?w=2560{{#roundelProperties}}&$roundel$&{{roundelConfig roundel}}{{/roundelProperties}} 2x"
                    media="(min-width: 1024px)">

            <source srcset="//{{image.defaultHost}}/i/{{image.endpoint}}/{{image.name}}{{#if seoText}}/{{escapeUrl seoText}}{{/if}}.webp?w=1024{{#roundelProperties}}&$roundel$&{{roundelConfig roundel}}{{/roundelProperties}} 1x, //{{image.defaultHost}}/i/{{image.endpoint}}/{{image.name}}{{#if seoText}}/{{escapeUrl seoText}}{{/if}}.webp?w=2048{{#roundelProperties}}&$roundel$&{{roundelConfig roundel}}{{/roundelProperties}} 2x"
                    media="(min-width: 768px)"
                    type="image/webp">
            <source srcset="//{{image.defaultHost}}/i/{{image.endpoint}}/{{image.name}}{{#if seoText}}/{{escapeUrl seoText}}{{/if}}?w=1024{{#roundelProperties}}&$roundel$&{{roundelConfig roundel}}{{/roundelProperties}} 1x, //{{image.defaultHost}}/i/{{image.endpoint}}/{{image.name}}{{#if seoText}}/{{escapeUrl seoText}}{{/if}}?w=2048{{#roundelProperties}}&$roundel$&{{roundelConfig roundel}}{{/roundelProperties}} 2x"
                    media="(min-width: 768px)">

            <source srcset="//{{image.defaultHost}}/i/{{image.endpoint}}/{{image.name}}{{#if seoText}}/{{escapeUrl seoText}}{{/if}}.webp?w=768{{#roundelProperties}}&$roundel$&{{roundelConfig roundel}}{{/roundelProperties}} 1x, //{{image.defaultHost}}/i/{{image.endpoint}}/{{image.name}}{{#if seoText}}/{{escapeUrl seoText}}{{/if}}.webp?w=1536{{#roundelProperties}}&$roundel$&{{roundelConfig roundel}}{{/roundelProperties}} 2x"
                    media="(max-width: 768px)"
                    type="image/webp">
            <source srcset="//{{image.defaultHost}}/i/{{image.endpoint}}/{{image.name}}{{#if seoText}}/{{escapeUrl seoText}}{{/if}}?w=768{{#roundelProperties}}&$roundel$&{{roundelConfig roundel}}{{/roundelProperties}} 1x, //{{image.defaultHost}}/i/{{image.endpoint}}/{{image.name}}{{#if seoText}}/{{escapeUrl seoText}}{{/if}}?w=1536{{#roundelProperties}}&$roundel$&{{roundelConfig roundel}}{{/roundelProperties}} 2x"
                    media="(max-width: 768px)">

            <img src="//{{image.defaultHost}}/i/{{image.endpoint}}/{{image.name}}{{#if seoText}}/{{escapeUrl seoText}}{{/if}}?w=1600{{#roundelProperties}}&$roundel$&{{roundelConfig roundel}}{{/roundelProperties}}"
                 srcset="//{{image.defaultHost}}/i/{{image.endpoint}}/{{image.name}}{{#if seoText}}/{{escapeUrl seoText}}{{/if}}.webp?w=1600{{#roundelProperties}}&$roundel$&{{roundelConfig roundel}}{{/roundelProperties}} 1x, //{{image.defaultHost}}/i/{{image.endpoint}}/{{image.name}}{{#if seoText}}/{{escapeUrl seoText}}{{/if}}.webp?w=3200{{#roundelProperties}}&$roundel$&{{roundelConfig roundel}}{{/roundelProperties}} 2x,
                 //{{image.defaultHost}}/i/{{image.endpoint}}/{{image.name}}{{#if seoText}}/{{escapeUrl seoText}}{{/if}}?w=1600{{#roundelProperties}}&$roundel$&{{roundelConfig roundel}}{{/roundelProperties}} 1x, //{{image.defaultHost}}/i/{{image.endpoint}}/{{image.name}}{{#if seoText}}/{{escapeUrl seoText}}{{/if}}?w=3200{{#roundelProperties}}&$roundel$&{{roundelConfig roundel}}{{/roundelProperties}} 2x"
                 class="amp-ca-image-pic" {{#if imageAltText}} alt="{{imageAltText}}" {{/if}} {{#if seoText}} title="{{seoText}}"{{/if}} />
        {{/if}}

</picture>
{{#iff display '==' 'Hotspots'}}
   <div class="amp-ca-image-actions" data-amp-image-actions="//{{image.defaultHost}}/i/{{image.endpoint}}/{{image.name}}.json?metadata=true&v="></div>
{{/iff}}
</div>

