<div class="amp-ca-blog js_ca_blog">
	{{#with snippet}}
    	<div class="amp-ca-blog-snippet-wrap">
        	{{dynamicTemplate [@type] ../renderTypes this}}
    	</div>
    {{/with}}
    <div class="amp-ca-blog-content">
        {{#each contentTypes}}
            <div class="amp-ca-block-wrap">{{dynamicTemplate [@type] ../renderTypes this}}</div>
        {{/each}}
    </div>
</div>
